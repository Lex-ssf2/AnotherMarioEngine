		public var m_currentStartPoint:Point = new Point();
		private static var ROOT:Main;
		public static var resourceManager:Object;
		public static var MAXPLAYERS:int = 2;
		public var m_loadedCharacters:int = 0;
		
		public var m_currentLoader:SpriteSheetLoader;
		public var m_currentLoaderEffects:SpriteSheetLoader;
		private var m_jsonLoader:URLLoader = new URLLoader();
		public var m_swfLoader:Array = new Array();
		
		public function Main() 
		{
			super();
			ROOT = this;
			if (stage) init();
			else addEventListener(Event.ADDED_TO_STAGE, init,false,0,true);
		}
		
		public function init(e:Event = null):void 
		{
			KeyObject.construct(stage);
			m_jsonLoader.load(new URLRequest("config/mappings.json"))
			m_jsonLoader.addEventListener(Event.COMPLETE, loadedResourceManager);
			removeEventListener(Event.ADDED_TO_STAGE, init);
			// entry point
		}
		
		private function firstMenu():void 
		{
			m_currentLoader = new SpriteSheetLoader(TilesetSkins.getBitmap(0), 16, 16, 8, 5);
			m_currentLoaderEffects = new SpriteSheetLoader(TilesetSkins.getBitmapParticles(0), 10, 11, 1, 1);
			saveData.initializeSaveData();
			m_levelEditor = new MenuEditor(m_TestMap);
			addChild(m_levelEditor);
		}
		
		private function loadedResourceManager(e:Event):void 
		{
			resourceManager = JSON.parse(m_jsonLoader.data);
			firstMenu();
			m_jsonLoader.removeEventListener(Event.COMPLETE, loadedResourceManager);
		}
		
		public function loadCharacterSWF(data:String):void 
		{
			m_swfLoader.push(new SWFLoader(new URLRequest(resourceManager.characters[data].file)));
			m_swfLoader[m_swfLoader.length - 1].load();
			m_swfLoader[m_swfLoader.length - 1].addEventListener(LoaderEvent.COMPLETE,sampleTest);
			m_swfLoader[m_swfLoader.length - 1].autoDispose = true;
		}
		
		private function sampleTest(e:LoaderEvent):void 
		{
			CharacterStats.writeStats(e.target.rawContent.getCharacter());
			e.target.removeEventListener(LoaderEvent.COMPLETE, sampleTest);
			m_loadedCharacters++
			if (m_loadedCharacters == m_swfLoader.length)
			{
				GameController.startGame(new Level(GameController.m_currentLevelData), 1);
				for (var i:int = 0; i < m_swfLoader.length; i++) 
				{
					m_swfLoader[i].dispose(true);
					m_swfLoader[i].unload();
					m_swfLoader[i] = null;
				}
				m_swfLoader.splice(0, m_swfLoader.length);
				m_loadedCharacters = 0;
			}
		}
		
		public function startEditor(levelData:Array):void 
		{
			System.gc();
			GameController.currentLevel = null;
			m_levelEditor = new MenuEditor(levelData);
			addChild(m_levelEditor);
		}
		
		public static function get Root():Main
        {
            return (ROOT);
        }
		
